{
  "name": "poikilotherm/certmgmt",
  "version": "0.1.0",
  "author": "Oliver Bertuch",
  "summary": "A puppet module to manage pregenerated ssl certificates and keys.",
  "license": "Apache-2.0",
  "source": "https://github.com/poikilotherm/puppet-certmgmt",
  "project_page": "https://github.com/poikilotherm/puppet-certmgmt",
